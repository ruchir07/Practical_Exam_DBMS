db.students.insertMany([
  { _id: 1, name: "Ravi", age: 21, marks: 88, subjects: ["Math", "Physics"], city: "Pune" },
  { _id: 2, name: "Sneha", age: 22, marks: 92, subjects: ["Chemistry", "Biology"], city: "Mumbai" },
  { _id: 3, name: "Arjun", age: 20, marks: 75, subjects: ["Math", "Chemistry"], city: "Delhi" },
  { _id: 4, name: "Priya", age: 23, marks: 65, subjects: ["Biology", "Physics"], city: "Pune" },
  { _id: 5, name: "Vikas", age: 21, marks: 55, subjects: ["Math", "English"], city: "Chennai" },
  { _id: 6, name: "Asha", age: 22, marks: 45, subjects: ["History", "English"], city: "Bangalore" }
